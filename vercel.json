{
  "framework": "nextjs",
  "regions": ["sin1"],
  "buildCommand": "npm run build",
  "devCommand": "next dev",
  "installCommand": "npm ci",
  "outputDirectory": ".next",
  "cleanUrls": true,
  "trailingSlash": false,
  "buildTimestamp": "2025-12-02T18:03:00+07:00",
  "github": {
    "autoAlias": false,
    "silent": false,
    "autoJobCancelation": false
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin/data/members",
      "destination": "/admin/members"
    },
    {
      "source": "/admin/data/sekbid",
      "destination": "/admin/sekbid"
    }
  ]
}
