{
  "framework": "nextjs",
  "regions": ["sin1"],
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin/data/members",
      "destination": "/admin/members"
    },
    {
      "source": "/admin/data/sekbid",
      "destination": "/admin/sekbid"
    }
  ]
}
